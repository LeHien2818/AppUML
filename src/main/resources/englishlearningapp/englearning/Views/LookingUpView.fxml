<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="englishlearningapp.englearning.Controller.LookingUpController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="900.0" pickOnBounds="true">
         <image>
            <Image url="@../src/image/Background.png" />
         </image>
      </ImageView>
      <Pane prefHeight="106.0" prefWidth="900.0">
         <children>
            <ImageView fitHeight="78.0" fitWidth="82.0" layoutX="14.0" layoutY="15.0" pickOnBounds="true">
               <image>
                  <Image url="@../src/image/trans-icon.png" />
               </image>
            </ImageView>
            <Button layoutX="182.0" layoutY="32.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="146.0" styleClass="classBtn" text="Search" />
            <Button layoutX="443.0" layoutY="32.0" mnemonicParsing="false" onAction="#clickGame" prefHeight="43.0" prefWidth="146.0" styleClass="classBtn" text="Game" />
            <Button layoutX="704.0" layoutY="33.0" mnemonicParsing="false" onAction="#clickTranslate" prefHeight="43.0" prefWidth="146.0" styleClass="classBtn" text="Translate" />
         </children>
      </Pane>
      <Pane layoutY="120.0" prefHeight="439.0" prefWidth="183.0">
         <children>
            <TextField fx:id="textInput" layoutX="46.0" layoutY="2.0" onKeyReleased="#inputWordHanddle" prefHeight="25.0" prefWidth="130.0" promptText="Word" />
         </children>
      </Pane>
      <TextArea fx:id="definitionArea" editable="false" layoutX="260.0" layoutY="158.0" prefHeight="410.0" prefWidth="549.0" styleClass="textBox">
         <font>
            <Font size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></TextArea>
   </children>
</AnchorPane>
