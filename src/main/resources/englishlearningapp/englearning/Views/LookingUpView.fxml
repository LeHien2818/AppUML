<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="searchRoot" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="englishlearningapp.englearning.Controller.LookingUpController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="900.0" pickOnBounds="true">
         <image>
            <Image url="@../src/image/Back.png" />
         </image>
      </ImageView>
      <Pane prefHeight="115.0" prefWidth="900.0">
         <children>
            <ImageView fitHeight="63.0" fitWidth="638.0" layoutX="254.0" layoutY="47.0" opacity="0.25" pickOnBounds="true">
               <image>
                  <Image url="@../src/image/MoonPurple.png" />
               </image>
            </ImageView>
            <Button fx:id="searchBtn" layoutX="254.0" layoutY="46.0" mnemonicParsing="false" prefHeight="63.0" prefWidth="175.0" styleClass="classBtn" text="Search" textFill="WHITE">
               <font>
                  <Font name="Eras Bold ITC" size="24.0" />
               </font></Button>
            <Button fx:id="gameBtn" layoutX="492.0" layoutY="46.0" mnemonicParsing="false" onAction="#clickGame" prefHeight="63.0" prefWidth="175.0" styleClass="classBtn" text="Game" textFill="WHITE">
               <font>
                  <Font name="Eras Bold ITC" size="24.0" />
               </font></Button>
            <Button fx:id="translateBtn" layoutX="724.0" layoutY="46.0" mnemonicParsing="false" onAction="#clickTranslate" prefHeight="63.0" prefWidth="166.0" styleClass="classBtn" text="Translate" textFill="WHITE">
               <font>
                  <Font name="Eras Bold ITC" size="24.0" />
               </font></Button>
         </children>
      </Pane>
      <Pane layoutX="37.0" layoutY="146.0" prefHeight="439.0" prefWidth="183.0">
         <children>
            <TextField fx:id="textInput" layoutX="19.0" layoutY="13.0" onKeyReleased="#inputWordHanddle" prefHeight="33.0" prefWidth="157.0" promptText="Search" />
         </children>
      </Pane>
      <TextArea fx:id="definitionArea" editable="false" layoutX="255.0" layoutY="166.0" prefHeight="399.0" prefWidth="631.0" styleClass="textBox">
         <font>
            <Font size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></TextArea>
      <Button layoutX="640.0" layoutY="126.0" mnemonicParsing="false" onAction="#onAddBtn" prefHeight="26.0" prefWidth="62.0" text="Add" />
      <Button layoutX="736.0" layoutY="126.0" mnemonicParsing="false" onAction="#onDelete" prefHeight="26.0" prefWidth="62.0" text="Delete" />
      <Button layoutX="823.0" layoutY="126.0" mnemonicParsing="false" onAction="#onCustom" prefHeight="26.0" prefWidth="62.0" text="Modify" />
   </children>
</AnchorPane>
